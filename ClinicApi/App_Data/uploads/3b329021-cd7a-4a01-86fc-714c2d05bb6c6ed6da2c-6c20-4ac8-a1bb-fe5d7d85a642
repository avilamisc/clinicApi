<script language="javascript">


    $(function () {

        function setupReviewCommentForm() {
            var commentSection = $(this);
            var commentsList = $(".reviewCommentList", this);
            var commentForm = $(".reviewCommentForm", this);
            var resultContainer = $(".commentResult", commentForm);
            var form = $("form", commentForm);
            var originalComment = $("#OriginalComment", commentForm);
            var text = $("#Text", commentForm);
            var deleteLink = $(".deleteComment a", this);
            var cancel = $(".cancel", commentForm);
            var list = $(".cancel", commentForm);

            function showReviewCommentForm(event) {
                event.preventDefault();
                toggleForm();
            }

            function toggleForm() {
                commentsList.toggle();
                commentForm.toggle();
                if (commentForm.is(':hidden')) {
                    text.val(originalComment.val());
                }
            }

            function submitReviewCallback(data, status, jqXHR) {
                if (data.Success) {
                    var refreshLocation = '/3b329021-cd7a-4a01-86fc-714c2d05bb6c/view/Reviews';
                    window.location = refreshLocation;
                }
                else {
                    resultContainer.empty();
                    resultContainer.append(data.ErrorMessage);
                    $("input", this).prop("disabled", false);
                }
            }

            commentSection.parents(".review").find("a.createComment").add("a.editComment", commentSection).click(showReviewCommentForm);
            cancel.click(toggleForm);
            form.submit(function () {
                Galleries.form.submitAsyncWithCallback(this, null, submitReviewCallback);
                $("input", this).prop("disabled", true);
                return false;
            });
            deleteLink.click(function () {
                var deleteForm = $(this).parents(".deleteComment>form");
                Galleries.form.submitAsyncWithCallback(deleteForm, null, function () { commentsList.empty(); });
                $("input", this).prop("disabled", true);
                text.val('');
                return false;
            });
        }
        
        $("#reviewList .reviewCommentSection").each(setupReviewCommentForm);

        attachSortHandler('SortBy', 'aSortBy', 0); 
    });

   function SetReviewHidden(reviewId, isHidden) {
       var form = $('#setReviewForm');
       $('#reviewId', form).val(reviewId);
       $('#isHidden', form).val(isHidden);

       form.submit();
   }

    function onStarMouseOver(id) {
        var id_prefix = id.substring(0, id.lastIndexOf('_') + 1);
        var index = parseInt(id.substring(id.lastIndexOf('_') + 1, id.length));

        for (i = 0; i < 5; i++) {
            var star = $('#' + id_prefix + i);
            if (i <= index)
                star.removeClass('EmptyRatingStar').addClass('FilledRatingStar');
            else
                star.removeClass('FilledRatingStar').addClass('EmptyRatingStar');
        }
    }
    function onStarsMouseOut() {
        var id_prefix = "Star_";
        var currentRating = parseInt($('#Rating').val());

        for (i = 0; i < 5; i++) {
            var star = $('#' + id_prefix + i.toString());
            if (i < currentRating)
                star.removeClass('EmptyRatingStar').addClass('FilledRatingStar');
            else
                star.removeClass('FilledRatingStar').addClass('EmptyRatingStar');
        }
    }
    function onStarClick(index) {
        $('#Rating').val(index + 1);
    }

    function displayCreateReview() {
        $('#reviewsTabCreate').hide();
        $('#newReview').show();
        $('#reviewList').hide();
        $('div.sortBy').hide();
    }
    function cancelCreateReview() {
        var refreshLocation = '/3b329021-cd7a-4a01-86fc-714c2d05bb6c/view/Reviews';
        window.location = refreshLocation;
    }
    function deleteReview() {
        var refreshLocation = '/3b329021-cd7a-4a01-86fc-714c2d05bb6c/deletereview';
        window.location = refreshLocation;
    }
</script>

